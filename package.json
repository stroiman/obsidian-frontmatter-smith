{
  "name": "frontmatter-smith",
  "version": "0.4.0",
  "description": "Frontmatter smith plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && eslint . && pnpm test && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "NODE_OPTIONS='--import tsx' mocha --extension js,ts",
    "test:watch": "NODE_OPTIONS='--import tsx' mocha -w -R min --extension js,ts",
    "vite": "vite"
  },
  "keywords": [],
  "author": "Peter Str√∏iman",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@rtvision/mocha-esbuild": "^0.1.8",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^16.18.104",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "builtin-modules": "3.3.0",
    "chai": "^4.4.1",
    "esbuild": "0.17.3",
    "esbuild-runner": "^2.2.2",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.4",
    "obsidian": "latest",
    "sinon": "^18.0.0",
    "sinon-chai": "^3.7.0",
    "tslib": "2.4.0",
    "tsx": "^4.16.2",
    "typescript": "5.5.4",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "fp-ts": "^2.16.8",
    "io-ts": "^2.2.21",
    "vanjs-core": "^1.5.1"
  }
}
